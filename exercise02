def infomation_list(name, last_name, year_of_birth, city, email, phone):
    print(f"{name} {last_name}, год рождения {year_of_birth}, "
          f"город {city}, почта {email}, телефон: {phone}")

user_name = input("Введите имя: ")
user_last_name= input("Введите фамилию: ")
user_year_of_birth = input("Введите год рождения: ")
user_city = input("Ведите ваш город: ")
user_email = input("Введите ваш e-mail: ")
user_phone = input("Введите ваш номер телефона: ")


infomation_list(name=user_name, last_name=user_last_name, year_of_birth=user_year_of_birth,
                city=user_city, email=user_email, phone=user_phone)
